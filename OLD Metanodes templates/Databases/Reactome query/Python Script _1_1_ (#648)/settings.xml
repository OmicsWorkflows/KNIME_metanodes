<entry key="sourceCode" type="xstring" value="from pandas import DataFrame%%00010%%00010# parse given accession ids from file and remove duplicates (also a single row might contain ids separated by a delimiter - parse them too)%%00010def remove_duplicate_ids(ids_column):%%00010    accession_ids_set = set()%%00010    for key, value in ids_column.iteritems():%%00010        for accession_id in value.split(flow_variables['proteinIDs_delimiter']):%%00010            accession_ids_set.add(accession_id)%%00010    return accession_ids_set%%00010    %%00010# create a single row with all ids (as in csv file) - it is POSTed this way to reactome, all ids in a single request%%00010def create_single_row(accession_ids_set):%%00010    single_row_accession_ids_list = [&quot;\n&quot;.join(accession_ids_set)]%%00010    return DataFrame(data=single_row_accession_ids_list, columns=[&quot;ids_column&quot;])%%00010%%00010def get_output_table():%%00010    ids_column = input_table.copy()[flow_variables['accession_id_column_name']]%%00010    accession_ids_set = remove_duplicate_ids(ids_column)%%00010    return create_single_row(accession_ids_set)%%00010%%00010output_table = get_output_table()"/>
